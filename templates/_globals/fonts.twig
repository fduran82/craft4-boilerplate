{% set baseUrl = alias("@web") ~ "/" %}

<link rel="preload"
  as="font"
  type="font/woff2"
  crossorigin="anonymous"
  href="{{ baseUrl }}fonts/inter-v3-latin-regular.woff2" />

<link rel="preload"
  as="font"
  type="font/woff2"
  crossorigin="anonymous"
  href="{{ baseUrl }}fonts/inter-v3-latin-700.woff2" />

<link rel="preload"
  as="font"
  type="font/woff2"
  crossorigin="anonymous"
  href="{{ baseUrl }}fonts/inter-v3-latin-600.woff2" />

<link rel="preload"
  as="font"
  type="font/woff2"
  crossorigin="anonymous"
  href="{{ baseUrl }}fonts/inter-v3-latin-500.woff2" />

<style>
  @font-face { font-family: "Inter var"; font-style: normal; font-weight: 400; font-display: swap; src: local("Inter
  var"), local("Inter var"), url("{{ baseUrl }}fonts/inter-v3-latin-regular.woff2") format("woff2"), url("{{baseUrl}}fonts/inter-v3-latin-regular.woff") format("woff"); }

  @font-face { font-family: "Inter var"; font-style: normal; font-weight: 700; font-display: swap; src: local("Inter
  var"), local("Inter var"), url("{{ baseUrl }}fonts/inter-v3-latin-700.woff2") format("woff2"), url("{{baseUrl}}fonts/inter-v3-latin-700.woff") format("woff"); }

  @font-face { font-family: "Inter var"; font-style: normal; font-weight: 600; font-display: swap; src: local("Inter
  var"), local("Inter var"), url("{{ baseUrl }}fonts/inter-v3-latin-600.woff2") format("woff2"), url("{{baseUrl}}fonts/inter-v3-latin-600.woff") format("woff"); }

  @font-face { font-family: "Inter var"; font-style: normal; font-weight: 500; font-display: swap; src: local("Inter
  var"), local("Inter var"), url("{{ baseUrl }}fonts/inter-v3-latin-500.woff2") format("woff2"), url("{{baseUrl}}fonts/inter-v3-latin-500.woff") format("woff"); }
</style>
